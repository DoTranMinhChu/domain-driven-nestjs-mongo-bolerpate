  Create<%= Name %>UseCase,
  DeleteOne<%= Name %>ByConditionUseCase,
  Fetch<%= Name %>sUseCase,
  GetOne<%= Name %>ByConditionUseCase,
  UpdateOne<%= Name %>ByConditionUseCase,
} from './use-cases';


@Module({
  imports: [Domain<%= Name %>Module],
  providers: [
